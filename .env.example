
# Application Port
PORT=3000

# Database (Cloud SQL PostgreSQL) - Use service account/ADC in production
# DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE?sslmode=require"

# JWT Secrets
JWT_SECRET="YOUR_SUPER_SECRET_KEY" # Change this in production!
JWT_EXPIRES_IN="3600s"

# Google Cloud Project ID
GCP_PROJECT_ID="your-gcp-project-id"

# Cloud Storage Bucket Name
GCS_BUCKET_NAME="your-cloud-storage-bucket-name"

# Vertex AI Endpoint ID (Example)
VERTEX_AI_ENDPOINT_ID="your_vertex_ai_endpoint_id"
VERTEX_AI_LOCATION="us-central1" # Or your region

# Gemini API Key (Manage securely, e.g., via Secret Manager)
# GEMINI_API_KEY="YOUR_GEMINI_API_KEY"